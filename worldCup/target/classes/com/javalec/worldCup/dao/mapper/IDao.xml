<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.javalec.worldCup.dao.IDao">
   <select id="list" resultType="com.javalec.worldCup.dto.Dto">
   	select * from ${value} order by win desc
   </select>
   <update id="update">
   	update ${param1} set win=win+1 where id=#{param2}
   </update>
</mapper>