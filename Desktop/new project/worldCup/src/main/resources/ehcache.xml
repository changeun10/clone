<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false"
	monitoring="autodetect" dynamicConfig="true">
	<diskStore path="java.io.tmpdir" />
	<cache name="statistic" maxElementsInMemory="1000"
		eternal="false" overflowToDisk="false" timeToLiveSeconds="1000"
		diskPersistent="false" memoryStoreEvictionPolicy="LRU" />
	<cache name="win" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="false" timeToLiveSeconds="1000" diskPersistent="false"
		memoryStoreEvictionPolicy="LRU" />
	<cache name="worldCup" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="false" timeToLiveSeconds="1000" diskPersistent="false"
		memoryStoreEvictionPolicy="LRU" />
</ehcache>
