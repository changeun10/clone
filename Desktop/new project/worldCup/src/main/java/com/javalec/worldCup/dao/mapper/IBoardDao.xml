<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javalec.worldCup.dao.IBoardDao">
   <select id="list" resultType="com.javalec.worldCup.dto.BoardDto">
   	select * from ${value}_board order by id desc
   </select>
   <insert id="write">
   	insert into ${title}_board values(${title}_seq.nextval,#{name},#{content},sysdate,#{writer})
   </insert>
 </mapper>